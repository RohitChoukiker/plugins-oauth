{
  "manifest_version": 3,
  "name": "AIRohit World",
  "version": "2.1.0",
  "description": "A stylish Chrome extension with Google OAuth login and a custom welcome screen.",
  "icons": {
    "48": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "AIRohit World"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "identity",
    "storage"
  ],
  "oauth2": {
    "client_id": "",
    "scopes": [
      "openid",
      "email",
      "profile",
      "https://www.googleapis.com/auth/userinfo.email"
    ]
  }
}
